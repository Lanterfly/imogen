{
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "env": {
    "es6": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "import/extensions": ["error", "always"],
    "indent": ["error", "tab"],
    "linebreak-style": "off",
    "no-lonely-if": "off",
    "no-tabs": "off"
  }
}